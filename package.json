{
  "name": "gaindeuk-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:analyze": "ANALYZE=true tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "analyze": "npm run build:analyze",
    "size": "npm run build && npx vite-bundle-analyzer dist",
    "analyze:deps": "node scripts/analyze-dependencies.js",
    "optimize": "npm run analyze:deps && npm run analyze"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.89.0",
    "axios": "^1.12.2",
    "lucide-react": "^0.544.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hot-toast": "^2.6.0",
    "react-router-dom": "^7.9.1",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/styled-components": "^5.1.34",
    "@vitejs/plugin-react": "^5.0.2",
    "eslint": "^9.35.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "terser": "^5.36.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.43.0",
    "vite": "^7.1.6",
    "vite-bundle-analyzer": "^0.11.0"
  }
}
